  "jsnext:main": "index",
  "files": [
    "_site",
    "dist",
    "index.es5.js",
    "index.js",
    "src",
    "src-es5"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/danvk/dygraphs.git"
  },
  "keywords": [
    "dygraphs",
    "javascript",
    "visualization",
    "canvas",
    "chart",
    "timeseries"
  ],
  "author": "Dan Vanderkam (danvdk@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danvk/dygraphs/issues"
  },
  "homepage": "https://github.com/danvk/dygraphs",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-strict-mode": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "browser-pack": "^6.1.0",
    "browserify": "^17.0.0",
    "chai": "^3.3.0",
    "clean-css": "^5.3.1",
    "core-js": "^3.25.1",
    "coveralls": "^3.1.1",
    "globals": "^13.17.0",
    "http-server": "^13.0.2",
    "istanbul": "^0.4.0",
    "jshint": "^2.5.10",
    "lcov-parse": "0.0.9",
    "mocha": "^2.1.0",
    "mocha-phantomjs": "3.5.3",
    "mocha-phantomjs-istanbul": "0.0.2",
    "parse-data-uri": "^0.2.0",
    "phantomjs": "^1.9.7-8",
    "phantomjs-function-bind-polyfill": "^1.0.0",
    "pre-commit": "^1.0.6",
    "source-map": "^0.6.1",
    "uglify-js": "^3.17.3",
    "watchify": "^4.0.0"
  },
  "scripts": {
    "build": "./scripts/build.sh",
    "clean": "git clean -dfx -e node_modules -e package-lock.json",
    "coverage": "./scripts/generate-coverage.sh",
    "site": "./scripts/run-site.sh",
    "prepack": "rm -rf _site && cp -Lr site _site && pax -rw -l LICENSE.txt README.md _site/ && jdupes -rL dist _site src src-es5",
